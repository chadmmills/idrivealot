= render 'tabs'
%table.list
  %thead
    %tr
      %th Date
      %th Route
      %th Mileage
      %th
  %tbody
    -  @mileage_records.each do |m|
      %tr
        %td
          = m.record_date.strftime("%m/%d")
        %td
          = m.route_description
        %td
          = m.end_mileage
        %td
          = link_to edit_mileage_record_path(m) do
            <i class="fa fa-edit edit-record"></i>
